{
  "name": "@duxe/create-nextjs-strapi-starter",
  "description": "A starter project for building websites backed with CMS. Built with NextJS v13.5.6 and Strapi v4.12.5",
  "version": "0.1.0",
  "bin": "./bin/cli.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"cd backend && npm run develop\" \"next dev\"",
    "dev:frontend": "next dev",
    "dev:backend": "npm run develop --prefix backend",
    "seed": "npm run seed --prefix backend",
    "export": "npm run export --prefix backend",
    "build": "next build",
    "start": "next start",
    "start:preview": "npx serve build",
    "lint": "next lint",
    "setup": "npm install && cd backend && npm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.6.7",
    "marked": "^12.0.0",
    "next": "13.5.6",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/marked": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "postcss": "^8",
    "serve": "^14.2.1",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
